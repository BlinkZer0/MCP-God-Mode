{
  "platform": "GPT",
  "description": "Configuration for GPT (OpenAI) AI platform",
  "proxy": {
    "http": "http://localhost:8080",
    "https": "http://localhost:8080"
  },
  "headers": {
    "x-target-url": "https://api.openai.com",
    "x-obfuscation-enabled": "true",
    "x-obfuscation-level": "moderate",
    "openai-version": "2024-02-15-preview",
    "User-Agent": "OpenAI/1.0 (Token Obfuscation Proxy)"
  },
  "environment": {
    "HTTPS_PROXY": "http://localhost:8080",
    "HTTP_PROXY": "http://localhost:8080",
    "NO_PROXY": "localhost,127.0.0.1",
    "OPENAI_PROXY": "http://localhost:8080"
  },
  "config_paths": {
    "windows": "%USERPROFILE%\\.openai\\config.json",
    "macos": "~/.openai/config.json",
    "linux": "~/.openai/config.json"
  },
  "environment_vars": [
    "OPENAI_API_KEY",
    "GPT_API_KEY"
  ],
  "endpoints": [
    "https://api.openai.com",
    "https://platform.openai.com"
  ],
  "user_agent_patterns": [
    "openai",
    "gpt",
    "chatgpt"
  ],
  "mcp_compatibility": {
    "supported": true,
    "mcp_version": "2024-11-05",
    "features": [
      "token_obfuscation",
      "proxy_middleware",
      "streaming_support",
      "function_calling"
    ]
  }
}
