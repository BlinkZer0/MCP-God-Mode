# Platform configuration
PLATFORM=desktop
# Options: desktop, android, ios

# Encryption settings
ENCRYPTION_KEY=your-encryption-passphrase-here
# Or leave empty to use OS keychain

# Appium settings (for mobile)
APPIUM_SERVER_URL=http://localhost:4723
ANDROID_DEVICE_NAME=emulator-5554
IOS_DEVICE_NAME=iPhone 15 Pro

# Playwright settings
PLAYWRIGHT_HEADLESS=false
PLAYWRIGHT_SLOW_MO=100

# Session storage
SESSION_STORAGE_PATH=./sessions
MACRO_STORAGE_PATH=./macros

# Provider settings
PROVIDER_CONFIG_PATH=./providers.json

# Debug settings
DEBUG=false
LOG_LEVEL=info
